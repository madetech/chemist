{
  "extends": "eslint-config-airbnb",
  "env": {
    "node": true
  },
  "rules": {
    "react/no-multi-comp": 0,
    "react/prop-types": 0,
    "react/jsx-filename-extension": 0,
    "import/default": 0,
    "import/no-duplicates": 0,
    "import/named": 0,
    "import/namespace": 0,
    "import/no-unresolved": 0,
    "import/no-named-as-default": 2,
    "comma-dangle": 0,
    "indent": [2, 2, { "SwitchCase": 1 }],
    "no-console": 0,
    "no-alert": 0,
    "semi": [2, "never"],
    "space-before-function-paren": [2, { "anonymous": "always", "named": "always" }],
    "no-empty": 0,
    "func-names": 0,
    "import/no-extraneous-dependencies": 0,
    "prefer-arrow-callback": 0,
    "no-param-reassign": 0,
    "no-underscore-dangle": 0,
    "generator-star-spacing": 0,
    "global-require": 0
  },
  "plugins": [
    "react",
    "import"
  ],
  "globals": {
    "describe": true,
    "it": true,
    "before": true,
    "after": true,
    "expect": true,
    "fetchival": true,
    "APP_HOST": true
  },
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 7,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true,
      "experimentalObjectRestSpread": true
    },
  }
}
